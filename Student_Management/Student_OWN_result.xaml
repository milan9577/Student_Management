        <Button x:Name="button" Content="Show" HorizontalAlignment="Left" Margin="66,72,0,0" VerticalAlignment="Top" Width="75" Background="#FF0CC6CF" Click="Show_btn"/>
        <Button x:Name="button_Copy1" Content="Back" HorizontalAlignment="Left" Margin="23,289,0,0" VerticalAlignment="Top" Width="75" Background="#FFBC6FD8" Click="Back_Button"/>
        <TextBlock x:Name="for_id" HorizontalAlignment="Left" Margin="223,71,0,0" TextWrapping="Wrap" Text="for_id" VerticalAlignment="Top" Background="#FF61D8D8" Height="21" Width="96"/>
        <TextBlock x:Name="for_dep" HorizontalAlignment="Left" Margin="495,75,0,0" TextWrapping="Wrap" Text="for_id" VerticalAlignment="Top" Background="#FF61D8D8" Height="17" Width="96"/>
